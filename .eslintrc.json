{
  "env": {
    "browser": true
  },
  "globals": {
    "$": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-underscore-dangle": [0]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
