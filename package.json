{
  "name": "accessibility.cloud",
  "version": "0.4.8",
  "description": "Fetches and shows places from accessibility.cloud on your website.",
  "author": "The accessibility.cloud team",
  "license": "MIT",
  "main": "accessibility.cloud.js",
  "homepage": "https://github.com/sozialhelden/accessibility-cloud-js",
  "bugs": {
    "email": "support@accessibility.cloud"
  },
  "repository": {
    "type": "git",
    "url": "sozialhelden/accessibility-cloud-js"
  },
  "keywords": [
    "accessibility",
    "wheelmap",
    "poi"
  ],
  "dependencies": {
    "c-3po": "0.0.4",
    "humanize-string": "^1.0.1",
    "mustache": "^2.3.0",
    "which": "^1.2.12"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-gettext-plugin": "^2.2.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-c-3po": "0.0.27",
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babili-webpack-plugin": "0.0.7",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "uglify-loader": "^1.3.0",
    "webpack": "^2.1.0-beta.28",
    "which": "^1.2.12"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base example/ --hot --inline --devtool eval --https --host 0.0.0.0",
    "build": "./node_modules/webpack/bin/webpack.js",
    "test": "node -v",
    "preversion": "npm test",
    "version": "export WP_LOCALE=de_DE; npm run build && cp dist/accessibility.cloud.${WP_LOCALE}.min.js dist/accessibility.cloud.${WP_LOCALE}-${npm_package_version}.min.js && rm dist/accessibility.cloud.${WP_LOCALE}.min.js && git add .",
    "postversion": "npm test"
  }
